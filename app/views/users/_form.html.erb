<%= form_for :user, url: signup_url do |f| %>
    <%= f.text_field :firstName, placeholder: t('.fname_prompt')  %>
    <%= f.text_field :lastName, placeholder: t('.lname_prompt')  %>
  <br>
  <%= f.text_field :email, placeholder: t('.email_prompt') %>
  <br>
    <%= f.password_field :password, placeholder: t('.pword_prompt') %>
    <%= f.password_field :password_confirmation, placeholder: t('.pword_prompt') %>
  <br>
  <%= f.submit "Submit"%>
<% end %>
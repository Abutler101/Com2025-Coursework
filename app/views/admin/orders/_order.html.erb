<tr>
  <td>
    <%= order.id %>
  </td>
  <td>
    <%=order.created_at %>
  </td>
  <td>
    <% order.order_entries.each do |i| %>
      <p><%=i.product.name%> x <%=i.quantity %></p>
    <%end %>
  </td>
  <td>
    <%= humanized_money_with_symbol order.totalCost %>
  </td>
  <td>
    <%=order.email%>
  </td>
  <td>
    <%= link_to fa_icon('trash'), [:admin, order], class: 'btn btn-sm btn-danger', method: :delete, data: { confirm: "Confirm?" } %>
  </td>
</tr>